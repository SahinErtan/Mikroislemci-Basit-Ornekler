.................... } 
....................  
.................... unsigned int8 lcd_read_nibble(void) 
.................... { 
....................   #if (defined(LCD_DATA4) && defined(LCD_DATA5) && defined(LCD_DATA6) && defined(LCD_DATA7)) 
....................    unsigned int8 n = 0x00; 
....................  
....................    /* Read the data port */ 
....................    n |= input(LCD_DATA4); 
....................    n |= input(LCD_DATA5) << 1; 
....................    n |= input(LCD_DATA6) << 2; 
....................    n |= input(LCD_DATA7) << 3; 
....................     
....................    return(n); 
....................   #else 
....................    return(lcd.data); 
*
0096:  MOVF   F83,W
0098:  MOVWF  00
009A:  SWAPF  00,W
009C:  ANDLW  0F
009E:  MOVWF  01
....................   #endif 
00A0:  RETURN 0
.................... } 
....................  
.................... void lcd_send_nibble(unsigned int8 n) 
.................... { 
....................   #if (defined(LCD_DATA4) && defined(LCD_DATA5) && defined(LCD_DATA6) && defined(LCD_DATA7)) 
....................    /* Write to the data port */ 
....................    output_bit(LCD_DATA4, bit_test(n, 0)); 
....................    output_bit(LCD_DATA5, bit_test(n, 1)); 
....................    output_bit(LCD_DATA6, bit_test(n, 2)); 
....................    output_bit(LCD_DATA7, bit_test(n, 3)); 
....................   #else       
....................    lcdlat.data = n; 
*
00DE:  SWAPF  45,W
00E0:  ANDLW  F0
00E2:  MOVWF  00
00E4:  MOVLW  0F
00E6:  ANDWF  F8C,W
00E8:  IORWF  00,W
00EA:  MOVWF  F8C
....................   #endif 
....................        
....................    delay_cycles(1); 
00EC:  NOP   
....................    lcd_output_enable(1); 
00EE:  BSF    F8C.0
....................    delay_us(2); 
00F0:  BRA    00F2
....................    lcd_output_enable(0); 
00F2:  BCF    F8C.0
00F4:  RETURN 0
.................... } 
....................  
.................... void lcd_send_byte(unsigned int8 address, unsigned int8 n) 
.................... { 
....................   #if defined(__PCB__) 
....................    set_tris_lcd(LCD_OUTPUT_MAP); 
....................   #else 
....................    lcd_enable_tris(); 
00F6:  BCF    F95.0
....................    lcd_rs_tris(); 
00F8:  BCF    F95.1
....................    lcd_rw_tris(); 
00FA:  BCF    F95.2
....................   #endif 
....................  
....................    lcd_output_rs(0); 
00FC:  BCF    F8C.1
....................    while ( bit_test(lcd_read_byte(),7) ) ; 
00FE:  RCALL  00A2
0100:  MOVFF  01,44
0104:  BTFSC  01.7
0106:  BRA    00FE
....................    lcd_output_rs(address); 
0108:  BCF    F8C.1
010A:  BTFSC  42.0
010C:  BSF    F8C.1
....................    delay_cycles(1); 
010E:  NOP   
....................    lcd_output_rw(0); 
0110:  BCF    F8C.2
....................    delay_cycles(1); 
0112:  NOP   
....................    lcd_output_enable(0); 
0114:  BCF    F8C.0
....................    lcd_send_nibble(n >> 4); 
0116:  SWAPF  43,W
0118:  MOVWF  44
011A:  MOVLW  0F
011C:  ANDWF  44,F
011E:  MOVFF  44,45
0122:  RCALL  00DE
....................    lcd_send_nibble(n & 0xf); 
0124:  MOVF   43,W
0126:  ANDLW  0F
0128:  MOVWF  44
012A:  MOVWF  45
012C:  RCALL  00DE
012E:  RETURN 0
.................... } 
