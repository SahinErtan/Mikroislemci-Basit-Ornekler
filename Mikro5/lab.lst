....................  
.................... unsigned int8 lcd_read_byte(void) 
.................... { 
....................    unsigned int8 low,high; 
....................  
....................  #if defined(__PCB__) 
....................    set_tris_lcd(LCD_INPUT_MAP); 
....................  #else 
....................   #if (defined(LCD_DATA4) && defined(LCD_DATA5) && defined(LCD_DATA6) && defined(LCD_DATA7)) 
....................    output_float(LCD_DATA4); 
....................    output_float(LCD_DATA5); 
....................    output_float(LCD_DATA6); 
....................    output_float(LCD_DATA7); 
....................   #else 
....................    lcdtris.data = 0xF; 
*
00A2:  MOVLW  0F
00A4:  ANDWF  F95,W
00A6:  IORLW  F0
00A8:  MOVWF  F95
....................   #endif 
....................  #endif 
....................          
....................    lcd_output_rw(1); 
00AA:  BSF    F8C.2
....................    delay_cycles(1); 
00AC:  NOP   
....................    lcd_output_enable(1); 
00AE:  BSF    F8C.0
....................    delay_cycles(1); 
00B0:  NOP   
....................    high = lcd_read_nibble(); 
00B2:  RCALL  0096
00B4:  MOVFF  01,45
....................        
....................    lcd_output_enable(0); 
00B8:  BCF    F8C.0
....................    delay_cycles(1); 
00BA:  NOP   
....................    lcd_output_enable(1); 
00BC:  BSF    F8C.0
....................    delay_us(1); 
00BE:  NOP   
....................    low = lcd_read_nibble(); 
00C0:  RCALL  0096
00C2:  MOVFF  01,44
....................        
....................    lcd_output_enable(0); 
00C6:  BCF    F8C.0
....................  
....................  #if defined(__PCB__) 
....................    set_tris_lcd(LCD_OUTPUT_MAP); 
....................  #else 
....................   #if (defined(LCD_DATA4) && defined(LCD_DATA5) && defined(LCD_DATA6) && defined(LCD_DATA7)) 
....................    output_drive(LCD_DATA4); 
....................    output_drive(LCD_DATA5); 
....................    output_drive(LCD_DATA6); 
....................    output_drive(LCD_DATA7); 
....................   #else 
....................    lcdtris.data = 0x0; 
00C8:  MOVLW  0F
00CA:  ANDWF  F95,W
00CC:  MOVWF  F95
....................   #endif 
....................  #endif 
....................  
....................    return( (high<<4) | low); 
00CE:  SWAPF  45,W
00D0:  MOVWF  00
00D2:  MOVLW  F0
00D4:  ANDWF  00,F
00D6:  MOVF   00,W
00D8:  IORWF  44,W
00DA:  MOVWF  01
