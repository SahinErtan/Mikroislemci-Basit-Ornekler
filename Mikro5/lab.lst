....................   #else 
....................    lcdtris.data = 0x0; 
0266:  MOVLW  0F
0268:  ANDWF  F95,W
026A:  MOVWF  F95
....................   #endif 
....................    lcd_enable_tris(); 
026C:  BCF    F95.0
....................    lcd_rs_tris(); 
026E:  BCF    F95.1
....................    lcd_rw_tris(); 
0270:  BCF    F95.2
0272:  CLRF   16
0274:  BTFSC  FF2.7
0276:  BSF    16.7
0278:  BCF    FF2.7
....................  #endif 
